{
  "name": "vott-messenger",
  "version": "0.11.2",
  "description": "An extensible framework for building Messenger bots",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "npm install && npm run test",
    "build": "cross-env BABEL_ENV=production babel src -d lib",
    "lint": "eslint ./src/",
    "test": "npm run lint && cross-env NODE_ENV=test nyc ava --verbose",
    "report": "nyc report --reporter=html",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/krismuniz/vott-messenger.git"
  },
  "author": "Kristian Mu√±iz <contact@krismuniz.com> (http://www.krismuniz.com)",
  "license": "MIT",
  "keywords": [
    "Messenger",
    "bot",
    "vott",
    "implementation",
    "conversational"
  ],
  "dependencies": {
    "body-parser": "1.17.0",
    "express": "4.15.0",
    "request": "2.79.0",
    "request-promise-native": "1.0.3",
    "vott": "1.8.2"
  },
  "devDependencies": {
    "ava": "0.18.2",
    "babel-cli": "6.22.1",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.22.0",
    "babel-register": "6.23.0",
    "coveralls": "2.11.16",
    "cross-env": "3.1.4",
    "nock": "9.0.9",
    "nyc": "10.1.0",
    "eslint": "3.17.0",
    "eslint-config-standard": "6.2.0",
    "eslint-plugin-promise": "3.4.2",
    "eslint-plugin-standard": "2.1.0"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "files": [
      "test/*.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  }
}
