{
  "name": "vott-messenger",
  "version": "0.7.2",
  "description": "An extensible framework for building Messenger bots",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "npm install && npm run test",
    "build": "cross-env BABEL_ENV=production babel src -d lib",
    "test": "standard && cross-env NODE_ENV=test nyc ava --verbose",
    "report": "nyc report --reporter=html",
    "try": "npm run build && node index",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/krismuniz/vott-messenger.git"
  },
  "author": "Kristian Mu√±iz <contact@krismuniz.com> (http://www.krismuniz.com)",
  "license": "MIT",
  "keywords": [
    "Messenger",
    "bot",
    "vott",
    "implementation",
    "conversational"
  ],
  "dependencies": {
    "body-parser": "1.15.2",
    "express": "4.14.0",
    "request": "2.75.0",
    "request-promise": "4.1.1",
    "vott": "1.5.0"
  },
  "devDependencies": {
    "ava": "0.15.2",
    "babel-cli": "6.11.4",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-preset-es2015": "6.9.0",
    "babel-register": "6.11.6",
    "coveralls": "2.11.12",
    "cross-env": "2.0.0",
    "nock": "8.0.0",
    "nyc": "7.1.0",
    "standard": "7.1.2"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "files": [
      "test/*.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "standard": {
    "ignore": [
      "/lib/"
    ]
  }
}
