{
  "name": "vott-messenger",
  "version": "0.8.0",
  "description": "An extensible framework for building Messenger bots",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "npm install && npm run test",
    "build": "cross-env BABEL_ENV=production babel src -d lib",
    "test": "standard && cross-env NODE_ENV=test nyc ava --verbose",
    "report": "nyc report --reporter=html",
    "try": "npm run build && node index",
    "prepublish": "npm run test && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/krismuniz/vott-messenger.git"
  },
  "author": "Kristian Mu√±iz <contact@krismuniz.com> (http://www.krismuniz.com)",
  "license": "MIT",
  "keywords": [
    "Messenger",
    "bot",
    "vott",
    "implementation",
    "conversational"
  ],
  "dependencies": {
    "body-parser": "1.15.2",
    "express": "4.14.0",
    "request": "2.76.0",
    "request-promise-native": "1.0.3",
    "vott": "1.6.1"
  },
  "devDependencies": {
    "ava": "0.16.0",
    "babel-cli": "6.18.0",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-es2015": "6.18.0",
    "babel-register": "6.18.0",
    "coveralls": "2.11.14",
    "cross-env": "3.1.3",
    "nock": "9.0.2",
    "nyc": "8.4.0",
    "standard": "8.5.0"
  },
  "ava": {
    "require": [
      "babel-core/register"
    ],
    "files": [
      "test/*.js"
    ]
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "standard": {
    "ignore": [
      "/lib/"
    ]
  }
}
